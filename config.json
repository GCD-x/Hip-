    "typography": {
        "fontFamily": "Monospace",
        "fontSize": {
            "heading1": "2rem",
            "heading2": "1.5rem",
            "body": "1rem"
        }
    },
    "alignment": {
        "vertical": ["Top", "Center", "Bottom"],
        "horizontal": ["Left", "Center", "Right"]
